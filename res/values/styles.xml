<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="FontW"><item name="android:textColor">#FFFFFF</item></style>
    <style name="FontG"><item name="android:textColor">#00FF00</item></style>
    <style name="FontR"><item name="android:textColor">#FF0000</item></style>
    <style name="FontY"><item name="android:textColor">#00FFFF</item></style>
    <style name="FontB"><item name="android:textColor">#0000FF</item></style>
    <style name="BGblack"><item name="android:background">#33000000</item></style>
    <style name="BGw"><item name="android:background">#FFFFFF</item></style>
    <style name="styleName">
        <item name="android:background">@style/BGblack</item>
    </style>
</resources>